{
  "compilerOptions": {
    "target": "es5",                        
    "module": "commonjs",                     
    "resolveJsonModule": true, 
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true 
  },
  "include": ["src/**/*", "knexfile.ts"]
}
